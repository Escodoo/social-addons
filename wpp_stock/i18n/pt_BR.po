#. module: wpp_stock
#: model:mail.template,body_html:wpp_stock.wpp_stock_template
msgid ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-"
"serif; font-size: 12px;\">\n"
"         <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"         <div>\n"
"            Hello *${object.partner_id.name or ''}*,\n"
"         </div>\n"
"         <div>\n"
"             % if object.state == 'draft':\n"
"                We have received your order *${object.name}*. It will be "
"shipped soon\n"
"                <div>\n"
"                    Order details are as follows: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.move_ids_without_package:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.product_uom_qty}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"             % endif\n"
"             % if object.state == 'confirmed':\n"
"                We have received your order *${object.name}*. It will be "
"shipped soon\n"
"                <div>\n"
"                    Order details are as follows: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.move_line_ids_without_package:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.product_uom_qty}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"             % endif\n"
"             % if object.state == 'assigned':\n"
"                Your order *${object.name}* is ready. It will be shipped "
"soon\n"
"                <div>\n"
"                    Order details are as follows: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.move_line_ids_without_package:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.product_uom_qty}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"             % endif\n"
"             % if object.state == 'done':\n"
"                Your order *${object.name}* is shipped.\n"
"                <div>\n"
"                    Order details are as follows: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.move_line_ids_without_package:\n"
"                            *Product: ${id.product_id.name}* <br>\n"
"                            *Qty: ${id.qty_done}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"             % endif\n"
"         </div>\n"
"         <div>\n"
"            If you have any questions, please feel free to contact us.\n"
"         </div>\n"
"            </p>\n"
"        \n"
"        "
msgstr ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-"
"serif; font-size: 12px;\">\n"
"         <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"         <div>\n"
"            Olá *${object.partner_id.name or ''}*,\n"
"         </div>\n"
"         <div>\n"
"             % if object.state == 'draft':\n"
"                Recebemos seu pedido *${object.name}*. Será enviado em "
"breve\n"
"                <div>\n"
"                    Os detalhes do pedido são os seguintes: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.move_ids_without_package:\n"
"                            *Produto: ${id.product_id.name}* <br>\n"
"                            *Qtd: ${id.product_uom_qty}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"             % endif\n"
"             % if object.state == 'confirmed':\n"
"                Recebemos seu pedido *${object.name}*. Será enviado em "
"breve\n"
"                <div>\n"
"                    Os detalhes do pedido são os seguintes: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.move_line_ids_without_package:\n"
"                            *Produto: ${id.product_id.name}* <br>\n"
"                            *Qtd: ${id.product_uom_qty}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"             % endif\n"
"             % if object.state == 'assigned':\n"
"                Seu pedido *${object.name}* está pronto. Será enviado em "
"breve\n"
"                <div>\n"
"                    Os detalhes do pedido são os seguintes: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.move_line_ids_without_package:\n"
"                            *Produto: ${id.product_id.name}* <br>\n"
"                            *Qtd: ${id.product_uom_qty}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"             % endif\n"
"             % if object.state == 'done':\n"
"                Seu pedido *${object.name}* foi enviado.\n"
"                <div>\n"
"                    Os detalhes do pedido são os seguintes: <br>\n"
"                    % for each in object:\n"
"                        % for id in each.move_line_ids_without_package:\n"
"                            *Produto: ${id.product_id.name}* <br>\n"
"                            *Qtd: ${id.qty_done}* <br>\n"
"                        % endfor\n"
"                    % endfor\n"
"                </div>\n"
"             % endif\n"
"         </div>\n"
"         <div>\n"
"            Se você tiver alguma dúvida, não hesite em contactar-nos.\n"
"         </div>\n"
"            </p>\n"
"        \n"
"        "

