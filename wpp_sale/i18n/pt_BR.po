#. module: wpp_sale
#: model:mail.template,body_html:wpp_sale.wpp_sales_template
msgid ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-"
"serif; font-size: 12px;\">\n"
"         <div>\n"
"            Hello *${object.partner_id.name or ''}*,\n"
"         </div>\n"
"         % if object.state == 'draft' or object.state == 'sent':\n"
"            Your Quotation *${object.name}* with amount *"
"${format_amount(object.amount_total, object.currency_id)}* is ready.\n"
"            <div>\n"
"                Your quotation date and time is \"${object."
"date_order}\"\n"
"            </div>\n"
"            <div>\n"
"                Quotation details are as follows: <br>\n"
"                % for each in object:\n"
"                    % for id in each.order_line:\n"
"                        *Product: ${id.product_id.name}* <br>\n"
"                         *Qty: ${id.product_uom_qty}*<br>\n"
"                         *Price: ${id.price_subtotal}*<br>\n"
"                    % endfor\n"
"                % endfor\n"
"            </div>\n"
"         % else:\n"
"            <div>\n"
"                Your Sale Order Number *${object.name}* with amount *"
"${format_amount(object.amount_total, object.currency_id)}* is "
"Confirmed.\n"
"            </div>\n"
"            <div>\n"
"                Your order date and time is \"${object.date_order}\"\n"
"            </div>\n"
"            <div>\n"
"                Your order details are as follows: <br>\n"
"                % for each in object:\n"
"                    % for id in each.order_line:\n"
"                        *Product: ${id.product_id.name}* <br>\n"
"                         *Qty: ${id.product_uom_qty}*<br>\n"
"                         *Price: ${id.price_subtotal}*<br>\n"
"                    % endfor\n"
"                % endfor\n"
"            </div>\n"
"         % endif\n"
"        <div>\n"
"            If you have any questions, please feel free to contact us.\n"
"        </div>\n"
"        \n"
"        "
msgstr ""
"<div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-"
"serif; font-size: 12px;\">\n"
"         <div>\n"
"            Olá *${object.partner_id.name or ''}*,\n"
"         </div>\n"
"         % if object.state == 'draft' or object.state == 'sent':\n"
"            Sua Cotação *${object.name}* with amount *"
"${format_amount(object.amount_total, object.currency_id)}* is ready.\n"
"            <div>\n"
"                A data e hora da cotação é \"${object.date_order}\"\n"
"            </div>\n"
"            <div>\n"
"                Os detalhes da cotação são os seguintes: <br>\n"
"                % for each in object:\n"
"                    % for id in each.order_line:\n"
"                        *Produto: ${id.product_id.name}* <br>\n"
"                         *Qtd: ${id.product_uom_qty}*<br>\n"
"                         *Preço: ${id.price_subtotal}*<br>\n"
"                    % endfor\n"
"                % endfor\n"
"            </div>\n"
"         % else:\n"
"            <div>\n"
"                Seu Número de Pedido de Venda *${object.name}* com "
"total *${format_amount(object.amount_total, object.currency_id)}* está "
"Confirmado.\n"
"            </div>\n"
"            <div>\n"
"                A data e hora do seu pedido são \"${object."
"date_order}\"\n"
"            </div>\n"
"            <div>\n"
"                Os detalhes do seu pedido são os seguintes: <br>\n"
"                % for each in object:\n"
"                    % for id in each.order_line:\n"
"                        *Produto: ${id.product_id.name}* <br>\n"
"                         *Qtd: ${id.product_uom_qty}*<br>\n"
"                         *Preço: ${id.price_subtotal}*<br>\n"
"                    % endfor\n"
"                % endfor\n"
"            </div>\n"
"         % endif\n"
"        <div>\n"
"            Se você tiver alguma dúvida, não hesite em contactar-nos.\n"
"        </div>\n"
"        \n"
"        "

